{
  "expo": {
    "name": "Passera",
    "slug": "passera",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.semrai.smartpass"
    },
    "android": {
      "package": "com.semrai.smartpass",
      "versionCode": 1
    },
    "entryPoint": "./src/entryPoint.tsx"
  }
}
